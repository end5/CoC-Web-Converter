{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "declaration": true,
        "outDir": "dist",
        "strict": true,
        "noUnusedLocals": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
    },
    "exclude": [
        "src/old",
        "tokenizer",
        "test",
        "dist"
    ]
}